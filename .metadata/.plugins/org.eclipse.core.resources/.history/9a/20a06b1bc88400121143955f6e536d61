package Model;

public class SlaveJob {
	
	private Slave mSlave;
	
	private int mPart;
	
	public SlaveJob(Slave slave, int part) {
		mSlave = slave;
		mPart = part;
	}
	
	public Slave getSlave() {
		return mSlave;
	}
	
	public int getPart() {
		return mPart;
	}

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((mSlave == null) ? 0 : mSlave.hashCode());
		result = prime * result + mPart;
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		SlaveJob other = (SlaveJob) obj;
		if (mSlave == null) {
			if (other.mSlave != null)
				return false;
		} else if (!mSlave.equals(other.mSlave))
			return false;
		if (mPart != other.mPart)
			return false;
		return true;
	}
	
	
}
