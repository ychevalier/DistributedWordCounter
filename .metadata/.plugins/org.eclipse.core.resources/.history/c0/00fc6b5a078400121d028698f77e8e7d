package Model;

public class Slave {
	
	private String mIP;
	private int mPort;
	
	private int mPart;
	
	public Slave(String ip, int port, int part) {
		mIP = ip;
		mPort = port;
		mPart = part;
	}
	
	public String getIP() {
		return mIP;
	}
	
	public int getPort() {
		return mPort;
	}
	
	public int getPart() {
		return mPart;
	}
}
