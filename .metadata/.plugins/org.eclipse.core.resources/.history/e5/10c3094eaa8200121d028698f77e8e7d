package System;

public class Utils {

	public static void Save(File file, String textToSave, int filesize) {

	    file.delete();
	    try {
	    	BufferedWriter out = new BufferedWriter(new FileWriter(file), 32768);
	        out.write(textToSave);
	        out.close();
	    } catch (IOException e) {
	    }
	}
}
