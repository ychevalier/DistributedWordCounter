package Network.Protocols;

public class ProtocolResultFM extends Protocol {
		
	// ======================
	// Master to Client
	// ======================
	
	// Slave Sends result.
	public static final int SLAVE_MAX_ARG_NUMBER = 3;
	public static final String SLAVE_SEND_RESULT = "RESULTFM";
	public static final String SLAVE_FILE_NAME = "name";
	public static final String SLAVE_RESULT_SIZE = "size";
	
	// Client answers.
	public static final String CLIENT_OK = "OK";
	public static final String CLIENT_KO = "KO";

}
