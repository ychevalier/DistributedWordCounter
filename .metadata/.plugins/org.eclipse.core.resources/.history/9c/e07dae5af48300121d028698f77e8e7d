package Model;

import java.util.HashMap;
import java.util.Map;

public class WordList extends SyncMap<String, Integer> {
	
	public synchronized void addEntry(String word, int number) {
		if (mMap.containsKey(word)) {
			mMap.put(word, mMap.get(word) + number);
		} else {
			mMap.put(word, number);
		}
	}

}
