package Model;

import java.util.Set;
import java.util.TreeSet;

public class SlaveList {
	
	Set<Slave> mSlaveList;
	
	public SlaveList() {
		mSlaveList = new TreeSet<Slave>();
	}
	
	public synchronized void addSlave(Slave s) {
		mSlaveList.add(s);
	}
	
	public synchronized void removeSlve(Slave s) {
		mSlaveList.remove(s);
	}

}
